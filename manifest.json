{
    "manifest_version" : 3,
    "version" : "1.1",
    "name" : "LeadTracker",
    "action" : {
        "default_popup" : "index.html",
        "default_icon" : "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*","https://*/*"],
            "css": ["./style.css"],
            "js": ["./index.js"]
        }
    ],
    "permissions" : [
        "tabs"
    
    ]
}